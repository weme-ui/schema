{
  "repos": [
    {
      "repo": "@weme-ui/weme-ui",
      "registry": "weme-ui/full",
      "directory": "test/fixtures/registry/full",
      "default": true
    }
  ],
  "paths": {
    "components": "~/components",
    "composables": "~/composables",
    "layouts": "~/layouts",
    "pages": "~/pages",
